{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run HackCsv-CSVUP",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/node_modules/@sap/html5-repo-mock/index.js",
      "args": [
        "--standalone",
        "/nsCSVUP/index.html",
        "--ui5",
        "{\"version\":\".\"}"
      ],
      "cwd": "${workspaceFolder}/CSVUP",
      "env": {
        "PORT": "6004",
        "MOCK_LOOKUP_DIRS": "[\"webapp\"]",
        "run.config": "{\"handlerId\":\"ui5_run_config_handler_id\",\"runnableId\":\"/home/user/projects/HackCsv/CSVUP\"}"
      },
      "envFile": "${workspaceFolder}/CSVUP/.env3",
      "preLaunchTask": "Build HackCsv"
    }
  ]
}